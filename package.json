    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npx tsx src/index.ts",
    "scratch": "npx tsx src/scratch.ts",
    "start:prod": "npx tsx build/index.js",
    "start:build": "yarn build:webpack && yarn start:prod",
    "dev": "npx tsx watch src/index.ts",
    "image:utils": "cmd.exe /k \"C:\\Program Files (x86)\\Windows Kits\\10\\Assessment and Deployment Kit\\Deployment Tools\\DandISetEnv.bat\"",
    "image:mount": "DISM /Mount-Image /ImageFile:\"F:\\mounts\\nnos\\WinPE_amd64\\media\\sources\\boot.wim\" /Index:1 /MountDir:\"C:\\mounts\\image\"",
    "image:unmount": "Dism /Unmount-Image /MountDir:\"C:\\mounts\\image\" /discard",
    "image:commit": "Dism /Commit-Image /MountDir:\"C:\\mounts\\image\"",
    "image:push": "..\\pushinimage.bat",
    "image:iso": "MakeWinPEMedia /ISO F:\\mounts\\nnos\\WinPE_amd64 F:\\mounts\\nnos\\image.iso",
    "build": "yarn build:ts && yarn build:webpack && yarn build:bin",
    "build:ts": "tsc",
    "build:webpack": "webpack-cli --config webpack.config.cjs",
    "build:bin": "yarn pkg",
    "patch": "node patches/patch.js",
    "pkg": "npx pkg . --harmony-top-level-await --no-bytecode",
    "pkg:test": "output/bin/mega-mind.exe"
  },
  "pkg": {
    "scripts": "output/bundled/*.js",
    "assets": [
      "output/bundled/**/*",
      "node_modules/**/*"
    ],
    "targets": [
      "node18-windows-x64"
    ],
    "options": [
      "experimental-modules",
      "experimental-top-level-await",
      "harmony-top-level-await"
    ],
    "outputPath": "./output/bin"
  },
